{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/submit-survey",
      "dest": "/index.js"
    },
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/public/(.*)",
      "dest": "/public/$1"
    }
  ]
}
